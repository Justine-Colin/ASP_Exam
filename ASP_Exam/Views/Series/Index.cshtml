}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.S_Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.S_Taille)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.S_Fini)
        </th>
