            @Html.DisplayNameFor(model => model.S_Complet)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.S_Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.S_Taille)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.S_Fini)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.S_Complet)
        </td>
        <td>
